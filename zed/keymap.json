[
  {
    "context": "Editor",
    "bindings": {
      "cmd-alt-right": "editor::SelectLargerSyntaxNode",
      "cmd-alt-left": "editor::SelectSmallerSyntaxNode",
      "cmd-e": "editor::Format",
      "cmd-k": "assistant::InlineAssist",
      "cmd-shift-l": ["workspace::SendKeystrokes", "cmd-c end enter console.log(\"value: \", cmd-v)"]
    }
  },
  {
    "context": "Editor && (vim_mode == normal || vim_mode == visual) && !VimWaiting && (extension==js || extension==jsx || extension==ts || extension==tsx )",
    "bindings": {
      "space l": ["workspace::SendKeystrokes", "escape v i w y o console.log(' cmd-v : ', cmd-v ) escape"]
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "j k": "vim::NormalBefore"
    }
  },
  {
    "context": "Editor && (vim_mode == normal || vim_mode == visual) && !VimWaiting && !menu",
    "bindings": {
      "space": null,
      "space e": "tab_switcher::Toggle",
      "space s f": "file_finder::Toggle",
      "space space": "command_palette::Toggle",
      "space o": "command_palette::Toggle",
      "space r": "editor::Rename",
      "space s v": "project_panel::ToggleFocus",
      "gcc": "vim::ToggleComments",
      "space x": ["workspace::SendKeystrokes", "escape \" _"],
      "space b": "editor::ToggleBreakpoint",
      "ctrl-l": "pane::ActivateNextItem",
      "ctrl-h": "pane::ActivatePreviousItem",
      "space a": "agent::ToggleFocus",
      "g d": ["workspace::SendKeystrokes", "g d z z"]
    }
  },
  {
    "context": "Editor && (vim_mode == visual && !VimWating && !menu)",
    "bindings": {
      "J": "editor::MoveLineUp",
      "K": "editor::MoveLineDown",
      "space p": ["workspace::SendKeystrokes", "\" _ d P"]
    }
  },
  {
    "context": "Editor && ((vim_mode == visual || vim_mode == normal) && !VimWating && !menu)",
    "bindings": {
      "space d": ["workspace::SendKeystrokes", "\" _ d"]
    }
  },
  {
    "context": "!Editor && TabSwitcher",
    "bindings": {
      "d": "tab_switcher::CloseSelectedItem",
      "j": ["workspace::SendKeystrokes", "down"],
      "k": ["workspace::SendKeystrokes", "up"],
      "space e": ["workspace::SendKeystrokes", "escape"]
    }
  },
  {
    "bindings": {
      "cmd-shift-d": "git_panel::ToggleFocus",
      "ctrl-alt-right": "pane::SplitRight",
      "ctrl-alt-left": "pane::SplitLeft",
      "ctrl-alt-up": "pane::SplitUp",
      "ctrl-alt-down": "pane::SplitDown"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "cmd-left": "project_panel::CollapseAllEntries"
    }
  }
]
